<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="apPrincipal" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.exacta.extratovisualfx.MenuController">   
    <children>            
          <VBox fx:id="vbMenu" alignment="TOP_CENTER" style="-fx-background-color: F5F5F5;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">               
              <children>                  
                  <ImageView fx:id="imgLogo" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../java/br/exacta/imagens/logo-extrato-sem-fundo.png" />
               </image>                  
                  </ImageView>                  
                  <Button fx:id="btnUsuarios" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Usuários">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnEquipamentos" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Equipamentos">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnCurrais" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Currais">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnIngredientes" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Ingredientes">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnReceitas" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Receitas">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnOrdemProducao" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Ordens de Produçao">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnProgramacao" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Programação">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>                  
                  <Button fx:id="btnAmostraResultados" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Resultados">                     
                      <VBox.margin>                        
                          <Insets bottom="10.0" />                     
                      </VBox.margin>                  
                  </Button>
                  <Button fx:id="btnSobre" mnemonicParsing="false" prefHeight="60.0" prefWidth="200.0" text="Sobre" />               
              </children>               
              <padding>                  
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />               
              </padding>            
          </VBox>
      <AnchorPane fx:id="apDados" minHeight="800.0" minWidth="800.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="270.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="50.0" />   
    </children>
</AnchorPane>
